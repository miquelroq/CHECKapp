<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alarm_doc">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ProfileSelection"
            android:label="@string/title_activity_profile_selection"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SelectDiagnostic" />
        <activity android:name=".DiagnoseCustomizer" />
        <activity android:name=".Unhealthy" />
        <activity android:name=".Healthy" />
        <activity android:name=".Checkup" />
        <activity android:name=".MainActivity">

        </activity>

        <service android:name=".DataGenerator" />
    </application>

</manifest>